    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="9" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.multipleapp.org.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.multipleapp.org.MultiplicationActivity" >
        </activity>
        <activity
            android:name="com.multipleapp.org.MultiplyActivity"
            android:label="@string/title_activity_multiply"
            android:parentActivityName="com.multipleapp.org" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.multipleapp.org.MainActivity" />
        </activity>
        <activity
            android:name="com.multipleapp.org.SIActivity"
            android:label="@string/title_activity_si"
