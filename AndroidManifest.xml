        </activity>
        <activity
            android:name="com.multipleapp.org.QuadraticActivity"
            android:label="@string/title_activity_quadratic"
            android:parentActivityName="com.multipleapp.org" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.multipleapp.org" />
        </activity>
        <activity
            android:name="com.multipleapp.org.SortingActivity"
            android:label="@string/title_activity_sorting"
            android:parentActivityName="com.multipleapp.org" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.multipleapp.org" />
        </activity>
    </application>

</manifest>